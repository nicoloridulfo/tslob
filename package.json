{
  "name": "tslob",
  "version": "1.0.0",
  "description": "An simple limit order book written in Typescript.",
  "main": "index.ts",
  "scripts": {
    "test": "./node_modules/.bin/ava ./tests/*.ts --verbose",
    "benchmark": "npx ts-node benchmarks/benchmark1.ts"
  },
  "author": "Nicolo Ridulfo",
  "license": "MIT",
  "dependencies": {
    "ava": "^3.15.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "ava": {
    "files": [
      "tests/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "failWithoutAssertions": false
  }
}